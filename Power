#include <iostream>

using namespace std;

int power(int first, int second){
   int ans=1;
    for(int i=0;i<second;i++){
        ans=ans*first;
    }
    return ans;
}

int main()
{
    int a,b;
    cout<< " enter a and b "<<endl;
    cin>> a >> b;
    
    int ans=power(a,b);
    cout<<"ans is "<<ans<<endl;

    return 0;
}
